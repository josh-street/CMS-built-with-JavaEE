<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE glassfish-web-app PUBLIC "-//GlassFish.org//DTD GlassFish Application Server 3.1 Servlet 3.0//EN" "http://glassfish.org/dtds/glassfish-web-app_3_0-1.dtd">
<glassfish-web-app error-url="">
  <security-role-mapping>
    <role-name>Voter</role-name>
    <group-name>Voter</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>Commenter</role-name>
    <group-name>Commenter</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>ApprovedUsers</role-name>
    <group-name>ApprovedUsers</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>UnapprovedUsers</role-name>
    <group-name>UnapprovedUsers</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>SysAdmin</role-name>
    <group-name>SysAdmin</group-name>
    <principal-name>streetjo</principal-name>
  </security-role-mapping>
  <resource-ref>
    <res-ref-name>comments</res-ref-name>
    <jndi-name>jdbc/rules</jndi-name>
  </resource-ref>
  <class-loader delegate="true"/>
  <jsp-config>
    <property name="keepgenerated" value="true">
      <description>Keep a copy of the generated servlet class' java code.</description>
    </property>
  </jsp-config>
</glassfish-web-app>
